if game.PlaceId == 4225025295 then
    local rf = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
    local win = rf:CreateWindow({
        Name = "Money Hub ",
        Icon = 0,
        LoadingTitle = "Loading Best op Ninja Simulator Script",
        LoadingSubtitle = "by Money",
        Theme = "Default",
        DisableRayfieldPrompts = false,
        DisableBuildWarnings = false,
        ConfigurationSaving = {
            Enabled = true,
            FolderName = "Luau", -- must be a string
            FileName = "Big Bro"
        },
        Discord = {
            Enabled = true,
            Invite = "KvvHREvB",
            RememberJoins = true
        },
        KeySystem = true,
        KeySettings = {
            Title = "Key System",
            Subtitle = "https://discord.gg/KvvHREvB",
            Note = "Join Discord to Get Key",
            FileName = "Key",
            SaveKey = true,
            GrabKeyFromSite = true,
            Key = {"MoneyProYe"}
        }
    })

    local Tab = win:CreateTab("Farm", 4483362458)
    local lp = game:GetService("Players").LocalPlayer
    local ws = workspace

    -------------------------------
    -- Auto Swing (Optimized)
    -------------------------------
    local autoActive = false
    local AutoSwingToggle = Tab:CreateToggle({
        Name = "Auto Swing",
        CurrentValue = false,
        Flag = "Toggle_AutoSwing",
        Callback = function(en)
            local char = lp.Character
            if not char then return end
            autoActive = en
            if en then
                -- Use a coroutine loop with a very short delay
                coroutine.wrap(function()
                    while autoActive do
                        local tool = char:FindFirstChildWhichIsA("Tool")
                        if tool then
                            pcall(function() tool:Activate() end)
                        end
                        task.wait(0.03)
                    end
                end)()
            end
        end,
    })

    -------------------------------
    -- Disable Rendering Toggle
    -------------------------------
    local orig = {}  -- store original LocalTransparencyModifier for each part
    local DisableRenderingToggle = Tab:CreateToggle({
        Name = "Disable Rendering",
        CurrentValue = false,
        Flag = "Toggle_DisableRendering",
        Callback = function(en)
            if en then
                for _,obj in pairs(ws:GetDescendants()) do
                    if obj:IsA("BasePart") then
                        orig[obj] = obj.LocalTransparencyModifier
                        obj.LocalTransparencyModifier = 1
                    end
                end
            else
                for _,obj in pairs(ws:GetDescendants()) do
                    if obj:IsA("BasePart") and orig[obj] ~= nil then
                        obj.LocalTransparencyModifier = orig[obj]
                        orig[obj] = nil
                    end
                end
            end
        end,
    })
end
