   local Toggle4 = Auto:CreateToggle({
   Name = "Auto Farm Coins(Very Bad Expext in Future Better)",
   CurrentValue = false,
   Flag = "Toggle4", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local HumanoidRootPart = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
   local Gravity = workspace.Gravity
   if Value then
   workspace.Gravity = 0
   con4 = game:GetService("RunService").Heartbeat:Connect(function()
   local closest = nil
   for _, v in pairs(workspace:GetDescendants()) do
   if v.Name == "Coin_Server" then
   local BodyVelocity = Instance.new("BodyVelocity",HumanoidRootPart)
BodyVelocity.MaxForce = Vector3.new(4000,4000,4000)
BodyVelocity.Velocity = (v.Position - HumanoidRootPart.Position).unit * 22
   end
   end
   end)
   else
   if con4 then
con4:Disconnect()
con4 = nil
workspace.Gravity = 187
HumanoidRootPart.BodyVelocity:Destroy()
end
end
end,
})
local Toggle5 = Tab:CreateToggle({
   Name = "GunESP",
   CurrentValue = false,
   Flag = "Toggle3", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   if Value then
   con5 = game:GetService("RunService").Heartbeat:Connect(function()
   for _, v in pairs(workspace:GetDescendants()) do
   if v.Name == "GunDrop" then
   if not v:FindFirstChild("Highlight") then
   local Highlight = Instance.new("Highlight",v)
   Highlight.FillColor = Color3.new(0,0,1)
   Highlight.Enabled = true
   end
   end
   end
   end)
   else
   if con5 then
   con5:Disconnect()
   for _, part in pairs(workspace:GetDescendants()) do
   if part.Name == "GunDrop" then
   if part:FindFirstChild("Highlight") then
   part.Highlight:Destroy()
   end
   end
   end
   end
   end
   end,
   })
   Rayfield:LoadConfiguration()
